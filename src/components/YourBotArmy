import React from "react";

function YourBotArmy({army}) {
    return(
        <div className="your-bot-army">
            <h2>My Bot Army</h2>
            <ul>
                {army.map((bot) => (
                    <li key={bot.id}>{bot.name}</li>
                ))}
            </ul>

        </div>
    )
}


export default YourBotArmy;